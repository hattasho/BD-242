sqlite> CREATE TABLE TB_CLIENTES (
(x1...> ID INTEGER PRIMARY KEY AUTOINCREMENT,
(x1...> ENDERECO VARCHAR(50) NOT NULL,
(x1...> NOME_CLI VARCHAR(100) NOT NULL
(x1...> );
sqlite>
sqlite> INSERT INTO TB_CLIENTES (ENDERECO, NOME_CLI) VALUES ('Av João Pessoa 2081', 'José Maria Alves');
sqlite> INSERT INTO TB_CLIENTES (ENDERECO, NOME_CLI) VALUES ('Rua Waldery Uchoa 4', 'Maria Conceição Tavares');
sqlite> INSERT INTO TB_CLIENTES (ENDERECO, NOME_CLI) VALUES ('Rua Padre Franscisco Pinto 790', 'João Cosme Fonseca');
sqlite>
sqlite> CREATE TABLE TB_VENDEDORES (
(x1...> ID INTEGER PRIMARY KEY AUTOINCREMENT,
(x1...> NOME VARCHAR(100) NOT NULL
(x1...> );
sqlite>
sqlite> INSERT INTO TB_VENDEDORES (NOME) VALUES ('Luciano Arruda Cavalcante');
sqlite> INSERT INTO TB_VENDEDORES (NOME) VALUES ('Joana Alves Pessoa');
sqlite> INSERT INTO TB_VENDEDORES (NOME) VALUES ('Mercia Bessa Santos');
sqlite> INSERT INTO TB_VENDEDORES (NOME) VALUES ('Antônio de Padua Lopes');
sqlite>
sqlite> CREATE TABLE TB_PRODUTOS (
(x1...> ID INTEGER PRIMARY KEY AUTOINCREMENT,
(x1...> CODIGO INTEGER,
(x1...> NOME VARCHAR(100) NOT NULL,
(x1...> PRECO_UNITARIO INTEGER
(x1...> );
sqlite>
sqlite> INSERT INTO TB_PRODUTOS (CODIGO, NOME, PRECO_UNITARIO) VALUES (100, 'Arroz Tio João', 6.00);
sqlite> INSERT INTO TB_PRODUTOS (CODIGO, NOME, PRECO_UNITARIO) VALUES (150, 'Feijão Carioquinha', 5.50);
sqlite> INSERT INTO TB_PRODUTOS (CODIGO, NOME, PRECO_UNITARIO) VALUES (200, 'Macarrão Fortaleza', 3.50);
sqlite> INSERT INTO TB_PRODUTOS (CODIGO, NOME, PRECO_UNITARIO) VALUES (250, 'Oleo de Soja', 4.00);
sqlite> INSERT INTO TB_PRODUTOS (CODIGO, NOME, PRECO_UNITARIO) VALUES (300, 'Manteiga Betania 500g', 8.00);
sqlite> INSERT INTO TB_PRODUTOS (CODIGO, NOME, PRECO_UNITARIO) VALUES (350, 'Queijo Ricota Betania', 7.00);
sqlite>
sqlite> CREATE TABLE TB_NOTAS_FISCAIS (
(x1...> NUM_NF INTEGER PRIMARY KEY,
(x1...> COD_CLI INTEGER,
(x1...> COD_VEN INTEGER,
(x1...> SERIE_NF TEXT
(x1...> );
sqlite>
