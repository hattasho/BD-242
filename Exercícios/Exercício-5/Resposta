sqlite> CREATE TABLE TB_ITENS_NOTAS_FISCAIS (
(x1...> NUM_NF INTEGER,
(x1...> COD_PRO INTEGER,
(x1...> QTD INTEGER,
(x1...> FOREIGN KEY (COD_PRO) REFERENCES TB_PRODUTOS (CODIGO),
(x1...> FOREIGN KEY (NUM_NF) REFERENCES TB_NOTAS_FISCAIS (NUM_NF)
(x1...> );
sqlite>
